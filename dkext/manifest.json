{
  "manifest_version": 2,
  "name": "DKExtender",
  "version": "0.1",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.doubleknot.com/manageevents/confirmedit.asp?*",
        "https://*.doubleknot.com/ManageEvents/ConfirmEdit.asp?*",
        "https://*.doubleknot.com/ManageEvents/EditActivity.asp"
      ],
      "js": ["class.js", "sessionevent.js"]
    },
    {
      "matches": [
	"https://*.doubleknot.com/manageevents/eventsmanagement.aspx?*"
      ],
      "js": ["programsession.js"]
    },
    {
      "matches": [
	"https://*.doubleknot.com/manageevents/viewregistrations.aspx?*"
      ],
      "js": ["viewregistrations.js"]
    }
  ]
}
